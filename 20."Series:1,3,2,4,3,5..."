#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Ingrese el número de términos (N): ";
    cin >> n;

    int impar = 1;
    int par = 2;

    for(int i = 1; i <= n; i++) {
        if(i % 4 == 1 || i % 4 == 3) {
            cout << impar << " ";
            impar += 2;
        } else {
            cout << par << " ";
            par += 2;
        }
    }

    return 0;
}
